{
  "name": "onvif-probe",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Simple cli tool for onvif discovery",
  "author": "an3park",
  "source": "src/index.ts",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/an3park/onvif-probe.git"
  },
  "keywords": [
    "onvif",
    "camera",
    "ip camera",
    "onvif-probe",
    "onvif-discovery",
    "onvif-scanner"
  ],
  "bin": {
    "onvif-probe": "./index.js"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "microbundle -f esm --target node --generateTypes false --sourcemap false"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "fast-xml-parser": "^5.0.8",
    "microbundle": "^0.15.1",
    "mri": "^1.2.0",
    "picocolors": "^1.1.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18"
  }
}
